{% extends 'base.html.twig' %}

{% block title %}Permiso{% endblock %}

{% block body %}
    <h1>Permiso</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Idpermiso</th>
                <td>{{ permiso.idpermiso }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ permiso.nombre }}</td>
            </tr>
            <tr>
                <th>Ruta</th>
                <td>{{ permiso.ruta }}</td>
            </tr>
            <tr>
                <th>Escritura</th>
                <td>{{ permiso.escritura ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Lectura</th>
                <td>{{ permiso.lectura ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_permiso_index') }}">back to list</a>

    <a href="{{ path('app_permiso_edit', {'idpermiso': permiso.idpermiso}) }}">edit</a>

    {{ include('permiso/_delete_form.html.twig') }}
{% endblock %}
